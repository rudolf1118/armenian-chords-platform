generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Artist {
  id     Int     @id @default(autoincrement())
  name   String  @unique
  songs  Song[]
}

model Song {
  id        Int     @id @default(autoincrement())
  title     String
  chords    String
  artist    Artist  @relation(fields: [artistId], references: [id])
  artistId  Int
}